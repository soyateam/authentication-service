    "@types/jsonwebtoken": "^8.3.8",
    "@types/passport": "^1.0.3",
    "amqplib": "^0.5.5",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "g": "^2.0.1",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
