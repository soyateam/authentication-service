{
  "name": "blue-bird-service-template",
  "version": "0.0.0",
  "description": "Service Template",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "dev": "ts-node src/index.ts",
    "start": "tsc && node ./dist/index.js",
    "lint": "eslint -c .eslintrc.json --no-error-on-unmatched-pattern ./src/**/*",
    "test": "npm run build && mocha --recursive  ./dist/**/*.spec.js",
    "test:unit": "mocha --recursive ./dist/**/*.manager.spec.js ./dist/**/*.repository.spec.js",
    "test:integration": "mocha --recursive ./dist/**/*.router.spec.js",
    "coverage": "nyc --reporter=html npm run test",
    "coverage:test": "nyc check-coverage --lines 95 --functions 95 --branches 95"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:yesodot/selenium/censorship-systems/blue-bird/blue-bird-service-template.git"
  },
